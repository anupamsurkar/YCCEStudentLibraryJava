<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.syrous.yccestudentlibraryjava">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/ycce"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.el.ElectricalHome" />
        <activity android:name=".ui.ee.ElectronicsHome" />
        <activity android:name=".ui.etc.EtcHome" />
        <activity android:name=".ui.mech.MechHome" />
        <activity android:name=".ui.ctech.CtechHome" />
        <activity android:name=".ui.it.ItHome" />
        <activity android:name=".ui.civil.CivilHome" />

        <activity android:name=".ui.splash.SplashActivity"
            android:label="@string/ycce"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.login.LoginActivity"/>
        <activity android:name=".ui.home.HomeActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>